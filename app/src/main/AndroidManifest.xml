<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.razan.MyCarTracks">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".SharedPrefsManager.MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="MyCarTracks"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Alarm.TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <!--
        <receiver android:process=":remote" android:name=".AlarmBroadcastReceiver"/>
        <receiver android:name=".AutoStartBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </receiver>
        ...
        -->

        <receiver android:name=".Alarm.AlarmReceiver"/>
        <service
            android:name=".Alarm.AlarmService"
            android:exported="false"></service>
        <service
            android:name=".Alarm.ExampleJobService"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <service
            android:name=".NotifyService"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".EventsActivity" />
        <activity android:name=".UpcomingActivity" />
        <activity android:name=".ServicesActivity" />
        <activity android:name=".OldEventsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>